FORTRESS_HOME="`${0%foreg}fortress_home`"

USE_EMACS=${USE_EMACS:-emacs}
[ -n "$EMACS" ] && unset EMACSLOADPATH
"${USE_EMACS}" -batch "$@" -l "$FORTRESS_HOME/Fortify/fortify.el" -f foreg
