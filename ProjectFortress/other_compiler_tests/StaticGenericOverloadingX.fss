export Executable

(* Generic top-level overloaded functions
   THIS WORKS
*)

f[\T\](x:String):ZZ32 = 2
f[\T extends ZZ32\](x: T):ZZ32 = 2 + x

run() = do
  a = f[\ZZ32\](3)      (*) 5
  b = f[\ZZ32\](8)      (*) 10
  c = f[\String\]("cat")     (*) 2

  println(
  if (a=5 AND b=10 AND c=2)
     then "pass" else "fail" end
  )
end
