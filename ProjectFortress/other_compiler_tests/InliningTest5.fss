component InliningTest5
export Executable


run(): () = do
       a = 1.0
       b = 2.0
       c = 3.0
       d = 4.0
       e = 5.0
       if |a - b| <= |c| d OR: e < c then                 
         println("true")
      else
        println("false")
      end
end
end
