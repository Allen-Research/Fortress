(* Abstract Functional Method, does it work? *)

component Afm1
export Executable

trait T
  n(self):String
end

object O(x:String) extends T
  n(self):String = x
end

m(x:T):String = "T"
m(x:O):String = "O"

run():() = do
  x:T = O("PASS")
  println(n(x))
  println(m(x))
end
end
