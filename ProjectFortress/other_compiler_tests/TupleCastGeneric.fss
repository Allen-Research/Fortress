export Executable

trait T
  getter asString():String
end

object O(s:String) extends T
  getter asString():String = s
end

tu:(O,O) = ( O("Hello"), O("World!") )

join[\U extends T\](a:U, b:U) = a.asString " " b.asString

run():() = 
  println( join[\T\](tu) )

