component nestedTransactions1

export Executable



test1():() = do
    var count : ZZ32 = 0
    for i <- 1 # 5 do
    atomic do
        for j <- 1 # 5 do
           atomic do count := count + 1 end
        end
    end
    end
   println("count = " || count)
   assert(count = 25)
   println("PASS")
end

run():()=do
    println("Starting test")
    test1()
end
end
