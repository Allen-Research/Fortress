export Executable

trait T
  getter z(): ZZ32 = 0
end

object O(x:ZZ32) extends T
  getter y(): String = "y"
  m() = do
    println z               (* 27 No *)
    println z()             (* 28 No *)
    println self.z          (* 29 Ok *)
    println self.z()        (* 30 No *)

    println x               (* 32 Ok *)
    println self.x          (* 33 Ok *)

    println y                   (* 35 No *)
    println y()                 (* 36 No *)
    println self.y              (* 37 Ok *)
    println self.y()            (* 38 No *)
  end
end

run() = ()
