component Compiled5.Elif
export Executable

(* Parallelism. Should execute recursive calls to fib in parallel. *)

fib(n: ZZ32): ZZ32 =
    if n=0 then 0
    elif n=1 then 1
    else fib(n-1) + fib(n-2) end

run(): () = println(fib 20)

end
