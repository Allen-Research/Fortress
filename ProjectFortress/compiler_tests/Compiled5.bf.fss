  export Executable
  trait foo
    doIt():String = "blah"
  end
  trait quux extends foo
    niceness():String = doIt()
  end

  someFunction():foo = object extends foo
                         helper():String = doIt()
                         helper2():String = helper()
                       end

  object bar extends foo
    otherThing():String = doIt()
  end
  run():() = ignore(bar.doIt())
