export Executable

(* Generic functional methods
   OVERLOADING HAS BEEN REMOVED *)

trait T
  g[\S extends ZZ32, U\](self):ZZ32 = 8
end

object O(x: ZZ32) extends T
end

gee(o:T):ZZ32 = g[\ZZ32,String\](o)

run() =do
  o = O(3)
  b = gee(o)  (*) 8

  println(
  if ( b=8 )
     then "pass" else "fail" end
  )
end
