component VarianceTest5

  export Executable

  trait Test[\covariant T\]
    getter x() : T
  end

  object Impl[\T\](x: T) extends Test[\T\]
  end

  trait Foo end
  object Bar() extends Foo end

  run() = do
    x: Test[\Foo\] = Impl(Bar())
    ()
  end

end