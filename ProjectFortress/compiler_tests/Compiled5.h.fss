import CompilerSystem.args
export { Executable }

f(x:ZZ32):ZZ32 = if x <= 1 then x else  1 + g(x-1) end

g(x:ZZ32):ZZ32 = if x = 0 then x else h(x) end
h(x:ZZ32):ZZ32 = f(x)

run(): () = do
  println f(10)
end
