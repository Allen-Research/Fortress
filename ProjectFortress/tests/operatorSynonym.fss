component operatorSynonym
export Executable

opr *(x:ZZ32, y:ZZ32) = x DOT y

run() = do
  assert(5 - 3, 5 - 3)
  assert(2 * 3, 3 ∗ 2)
  assert(4 DOT 3, 3 ⋅ 4)
  assert(7 TIMES 2, 2 CROSS 7)
end

end
