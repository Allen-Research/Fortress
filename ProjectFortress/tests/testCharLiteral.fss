component testCharLiteral

export Executable

run():()=do
    foo = 'a'
    bar = 'a'
    baz = 'b'
    assert(foo, bar, "Character Compare")
    deny(foo, baz, "Character Mismatch")
    println("Now I know my " foo baz 'c' "s")
end
end
